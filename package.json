{
  "name": "gitstorm",
  "displayName": "GitStorm",
  "description": "PhpStorm-style Git panel with branches, commits, and file changes",
  "version": "0.1.0",
  "publisher": "gitstorm",
  "repository": {
    "type": "git",
    "url": "https://github.com/meepak/gitstorm"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitstorm.open",
        "title": "Open GitStorm"
      },
      {
        "command": "gitstorm.refresh",
        "title": "Refresh Git Data"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm run compile:ts && npm run copy:webview",
    "compile:ts": "tsc -p ./",
    "copy:webview": "copyfiles -u 1 \"src/webview/**/*\" out/ && copyfiles -u 1 \"src/templates/*\" out/ && copyfiles -u 1 \"src/styles/*\" out/ && copyfiles -u 1 \"src/assets/svg/*\" out/",
    "watch": "npm run copy:webview && tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4",
    "copyfiles": "^2.4.1"
  },
  "dependencies": {
    "simple-git": "^3.19.1"
  }
}
