{
  "name": "gitstorm-panel",
  "displayName": "GitStorm Panel",
  "description": "PhpStorm-style Git panel with branches, commits, and file changes",
  "version": "0.1.0",
  "publisher": "gitstorm",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitstorm.openPanel",
        "title": "Open GitStorm Panel"
      },
      {
        "command": "gitstorm.refresh",
        "title": "Refresh Git Data"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "simple-git": "^3.19.1"
  }
}
